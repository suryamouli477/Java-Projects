package p1;
//import p2.*;
import java.util.*;
public class Sums
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the n value:");
		 int n = s.nextInt();
		 int sum=0;
			
			for(int i=1;i<=n;i++)
			{
				sum=sum+i;
				System.out.println(sum);
				//System.out.print(i+" ");
				
			}
			if(sum%3==0||sum%5==0)
			{
				System.out.println("sum is ivisible by 3 and 5");
			}
			else
			{
				System.out.println("sum is not divisible:");
			}
			
		

		// NaturalNo ob=new NaturalNo();
		 //ob.calculateSum();
		 
		
	}
}