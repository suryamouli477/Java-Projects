package p1;
import java.util.*;
public class NaturalNo {
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the n value:");
		int n=s.nextInt();
		int i;
		int sum=0;
		for(i=1;i<=n;i++)
		{
			sum=(n*(n+1)/2);
			System.out.println(sum);
			//System.out.print(i+" ");
			
		}
		if(sum%5==0 || sum%5==5)
		{
			System.out.println("sum is ivisible by 5");
		}
		
		
	}
}