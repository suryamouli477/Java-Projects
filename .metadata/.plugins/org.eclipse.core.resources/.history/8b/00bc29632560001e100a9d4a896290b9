package p1;

public class EmployeeBLC {
	private int  empnumber;
	private String empname;
	private double empsal;
	EmployeeBLC( int empnumber, String empname, double empsal)
	{
		this.empnumber=empnumber;
		this.empname=empname;
		this.empsal=empsal;
	}
	public final int getEmpnumber() {
		return empnumber;
	}
	public final void setEmpnumber(int empnumber) {
		this.empnumber = empnumber;
	}
	public final String getEmpname() {
		return empname;
	}
	public final void setEmpname(String empname) {
		this.empname = empname;
	}
	public final double getEmpsal() {
		return empsal;
	}
	public final void setEmpsal(double empsal) {
		this.empsal = empsal;
	
	}
	public void dis()
	{
		EmployeeBLC ob=new EmployeeBLC(empnumber, empname, empsal);
	}
}
