package p1;
public class A{
	public int a;
	public static int b;
	public void m1()
	{
		System.out.println("pclass m1");
		System.out.println("a value is:"+a);
		System.out.println("b value is:"+b);
	
	}
	public static void  m2()
	{
		System.out.println("pclass m1");
		//System.out.println("a value is:"+a);
		System.out.println("b value is:"+b);
	}	
	
}
