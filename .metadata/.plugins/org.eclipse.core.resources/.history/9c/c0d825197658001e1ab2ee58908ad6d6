package p2;

public class Sum {
	public static void calculateSum()
	{
		int i;
		int sum=0;
		for(i=1;i<=n;i++)
		{
			sum=(n*(n+1)/2);
			System.out.println(sum);
			//System.out.print(i+" ");
			
		}
		if(sum%3==0 || sum%5==0)
		{
			System.out.println("sum is ivisible by 3 and 5");
		}
		else
		{
			System.out.println("sum is not divisible:");
		}
		
	}

}
