package p2;

public class Sum {
	public static void calculateSum()
	{
		
		
		int sum=0;
		int n=0;
		for(int i=1;i<=n;i++)
		{
			sum=sum+i;
			System.out.println(sum);
			//System.out.print(i+" ");
			
		}
		if(sum%3==0 || sum%5==0)
		{
			System.out.println("sum is ivisible by 3 and 5");
		}
		else
		{
			System.out.println("sum is not divisible:");
		}
		
	}

}
