package p1;
import java.util.*;
public class Assign1 {
	public static void main(String args[])
	{
		Scanner s= new Scanner(System.in);
		System.out.println("enter a value:");
		int a=s.nextInt();
		System.out.println("enter b value:");
		int b=s.nextInt();
		for(int i=a;i>b;i--)
		{
			System.out.println(i+" ");
		}
		 
	}

}
