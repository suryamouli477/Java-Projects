package p1;

public class A {
	public int a;
	public static int b;
	public void m1()
	{
		System.out.println("***pcm1()***");
		System.out.println("a value"+a);
		System.out.println("b value"+b);
	}
	public static void m2() {
		System.out.println("***pcm2()***");
		System.out.println("a value"+a);
		System.out.println("b value"+b);
	}
		
	}


