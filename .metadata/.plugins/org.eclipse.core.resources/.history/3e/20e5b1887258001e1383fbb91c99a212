package p1;
import java.util.*;
public class NaturalNo {
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the n value:");
		int n=s.nextInt();
		int i;
		int sum=0;
		for(i=1;i<=n;i++)
		{
			sum=sum+i;
			System.out.println("the sum of numbers:"+sum);
			//System.out.print(i+" ");
		}
		
	}
}