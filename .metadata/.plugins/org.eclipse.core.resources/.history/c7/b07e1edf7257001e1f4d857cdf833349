package p1;
import java.util.*;
public class Prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner s=new Scanner(System.in);
System.out.println("enter n numbers");
int n=s.nextInt();
int count =0;
int i;
for( i=0;i<=n;i++)
	
{
	for(int j=0;i<=j;j++)
	{
		if(i%j==0)
		{
			count++;
		}
	}
	
}
if (count==2)
{
	System.out.println(i);
}
else
	System.out.println(" ");
	}

}
