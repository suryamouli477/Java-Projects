package p1;

public class B extends A {
	public int x;
	public static int y;
	public void m3()
	{
		System.out.println("***m3()***");
		System.out.println("a value"+x);
		System.out.println("b value"+y);
	}
		public static void m4()
		{
			System.out.println("***m4()***");
			//System.out.println("a value"+x);
			System.out.println("b value"+y);
}
}